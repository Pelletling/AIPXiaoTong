
@using AIPXiaoTong.Model.Site;
@using AIPXiaoTong.Model;
@using Framework.Common;

@{


}
@model OrderPaid

<div class="page-container page-container-custom">
    <form action="" method="post" class="form form-horizontal" id="CreateFrom">

        <div class="panel panel-default">
            <div class="panel-header">基本信息</div>
            @if (Model.Merchant.AccountBank == AccountBankOption.GuangDa.ToInt())
            {
                <div class="panel-body">
                    <div class="panel-body">
                        <div class="row cl">
                            <label class="form-label col-xs-3 col-sm-3">订单编号：</label>
                            <div class="formControls col-xs-3 col-sm-3">
                                <strong>@Model.OrderNumber</strong>
                            </div>

                        </div>

                        <div class="row cl">
                            <label class="form-label col-xs-3 col-sm-3">项目名称：</label>
                            <div class="formControls col-xs-3 col-sm-3">
                                <strong>@Model.ProjectName</strong>
                            </div>


                            <label class="form-label col-xs-2 col-sm-2">商户信息：</label>
                            <div class="formControls col-xs-4 col-sm-4">
                                <strong>@Model.Merchant.Name</strong>
                            </div>
                        </div>

                        <div class="row cl">
                            <label class="form-label col-xs-3 col-sm-3">订单金额：</label>
                            <div class="formControls col-xs-3 col-sm-3">
                                <strong>@Model.TransactionAmount</strong>
                            </div>

                            <label class="form-label col-xs-2 col-sm-2">订单状态：</label>
                            <div class="formControls col-xs-4 col-sm-4">
                                <strong>@Model.StatusDesc</strong>
                            </div>
                        </div>

                        <div class="row cl">
                            <label class="form-label col-xs-3 col-sm-3">客户姓名：</label>
                            <div class="formControls col-xs-3 col-sm-3">
                                <strong>@Model.MemberName</strong>
                            </div>

                            <label class="form-label col-xs-2 col-sm-2">手机号：</label>
                            <div class="formControls col-xs-4 col-sm-4">
                                <strong>@Model.MemberMobile</strong>
                            </div>
                        </div>

                        <div class="row cl">
                            <label class="form-label col-xs-3 col-sm-3">身份证号：</label>
                            <div class="formControls col-xs-3 col-sm-3">
                                <strong>@Model.MemberIDNumber</strong>
                            </div>
                        </div>

                        <div class="row cl">
                            <label class="form-label col-xs-3 col-sm-3">操作员：</label>
                            <div class="formControls col-xs-3 col-sm-3">
                                <strong>@Model.EmployeeName</strong>
                            </div>

                            <label class="form-label col-xs-2 col-sm-2">设备SN号：</label>
                            <div class="formControls col-xs-4 col-sm-4">
                                <strong>@Model.EquipmentSNNo</strong>
                            </div>
                        </div>

                        <div class="row cl">
                            <label class="form-label col-xs-3 col-sm-3">创建时间：</label>
                            <div class="formControls col-xs-3 col-sm-3">
                                <strong>@Model.CreateDatetime</strong>
                            </div>

                            <label class="form-label col-xs-2 col-sm-2">备注：</label>
                            <div class="formControls col-xs-4 col-sm-4">
                                <strong>@Model.Remark</strong>
                            </div>
                        </div>

                    </div>
                </div>
            }
            else if (Model.Merchant.AccountBank == AccountBankOption.PingAn.ToInt())
            {
                <div class="panel-body">
                    <div class="panel-body">
                        <div class="row cl">
                            <label class="form-label col-xs-3 col-sm-3">订单编号：</label>
                            <div class="formControls col-xs-3 col-sm-3">
                                <strong>@Model.OrderNumber</strong>
                            </div>

                        </div>

                        <div class="row cl">
                            <label class="form-label col-xs-3 col-sm-3">项目名称：</label>
                            <div class="formControls col-xs-3 col-sm-3">
                                <strong>@Model.ProjectName</strong>
                            </div>


                            <label class="form-label col-xs-2 col-sm-2">商户信息：</label>
                            <div class="formControls col-xs-4 col-sm-4">
                                <strong>@Model.Merchant.Name</strong>
                            </div>
                        </div>

                        <div class="row cl">
                            <label class="form-label col-xs-3 col-sm-3">订单金额：</label>
                            <div class="formControls col-xs-3 col-sm-3">
                                <strong>@Model.TransactionAmount</strong>
                            </div>

                            <label class="form-label col-xs-2 col-sm-2">订单状态：</label>
                            <div class="formControls col-xs-4 col-sm-4">
                                <strong>@Model.StatusDesc</strong>
                            </div>
                        </div>

                        <div class="row cl">
                            <label class="form-label col-xs-3 col-sm-3">客户姓名：</label>
                            <div class="formControls col-xs-3 col-sm-3">
                                <strong>@Model.MemberName</strong>
                            </div>

                            <label class="form-label col-xs-2 col-sm-2">手机号：</label>
                            <div class="formControls col-xs-4 col-sm-4">
                                <strong>@Model.MemberMobile</strong>
                            </div>
                        </div>

                        <div class="row cl">
                            <label class="form-label col-xs-3 col-sm-3">身份证号：</label>
                            <div class="formControls col-xs-3 col-sm-3">
                                <strong>@Model.MemberIDNumber</strong>
                            </div>
                        </div>

                        <div class="row cl">
                            <label class="form-label col-xs-3 col-sm-3">操作员：</label>
                            <div class="formControls col-xs-3 col-sm-3">
                                <strong>@Model.EmployeeName</strong>
                            </div>

                            <label class="form-label col-xs-2 col-sm-2">设备SN号：</label>
                            <div class="formControls col-xs-4 col-sm-4">
                                <strong>@Model.EquipmentSNNo</strong>
                            </div>
                        </div>

                        <div class="row cl">
                            <label class="form-label col-xs-3 col-sm-3">银行订单号：</label>
                            <div class="formControls col-xs-3 col-sm-3">
                                <strong>@Model.PingAnOrderPaid.BankOrderNo</strong>
                            </div>

                            <label class="form-label col-xs-2 col-sm-2">止付天数：</label>
                            <div class="formControls col-xs-4 col-sm-4">
                                <strong>@Model.PingAnOrderPaid.FreezeTimeLimit</strong>
                            </div>
                        </div>

                        <div class="row cl">
                            <label class="form-label col-xs-3 col-sm-3">创建时间：</label>
                            <div class="formControls col-xs-3 col-sm-3">
                                <strong>@Model.CreateDatetime</strong>
                            </div>

                            <label class="form-label col-xs-2 col-sm-2">备注：</label>
                            <div class="formControls col-xs-4 col-sm-4">
                                <strong>@Model.Remark</strong>
                            </div>
                        </div>

                    </div>
                </div>
            }
        </div>

        <br />
        <div class="panel panel-default">
            <div class="panel-header">交易信息</div>
            <div class="panel-body">
                @if (Model.Merchant.AccountBank == AccountBankOption.GuangDa.ToInt())
                {
                    <div class="panel-body">

                        @if (Model.Status == OrderPaidStatusOption.Repeal.ToInt() || Model.Status == OrderPaidStatusOption.Cancel.ToInt() || Model.Status == OrderPaidStatusOption.PaySuccess.ToInt() || Model.Status == OrderPaidStatusOption.PayWait.ToInt())
                        {
                            <div class="row cl">
                                <label class="form-label col-xs-3 col-sm-3">交易状态：</label>
                                <div class="formControls col-xs-3 col-sm-3">
                                    <strong>@Model.StatusDesc</strong>
                                </div>

                                <label class="form-label col-xs-2 col-sm-2">交易金额：</label>
                                <div class="formControls col-xs-4 col-sm-4">
                                    <strong>@Model.TransactionAmount</strong>
                                </div>
                            </div>
                        }
                        else if (Model.Status == OrderPaidStatusOption.FrozenSuccess.ToInt() || Model.Status == OrderPaidStatusOption.FrozenWait.ToInt())
                        {
                            <div class="row cl">
                                <label class="form-label col-xs-3 col-sm-3">交易状态：</label>
                                <div class="formControls col-xs-3 col-sm-3">
                                    <strong>@Model.OrderPaidFreeze.StatusDesc</strong>
                                </div>

                                <label class="form-label col-xs-2 col-sm-2">平台流水号：</label>
                                <div class="formControls col-xs-4 col-sm-4">
                                    <strong>@Model.OrderPaidFreeze.SerialNumber</strong>
                                </div>
                            </div>

                            <div class="row cl">
                                <label class="form-label col-xs-3 col-sm-3">冻结金额:</label>
                                <div class="formControls col-xs-3 col-sm-3">
                                    <strong>@Model.OrderPaidFreeze.Amount</strong>
                                </div>

                                <label class="form-label col-xs-2 col-sm-2">账户余额：</label>
                                <div class="formControls col-xs-4 col-sm-4">
                                    <strong>@Model.OrderPaidFreeze.Balance</strong>
                                </div>
                            </div>

                            <div class="row cl">
                                <label class="form-label col-xs-3 col-sm-3">交易时间：</label>
                                <div class="formControls col-xs-3 col-sm-3">
                                    <strong>@Model.OrderPaidFreeze.TransTime</strong>
                                </div>
                            </div>

                        }
                        else if (Model.Status == OrderPaidStatusOption.UnFrozenSuccess.ToInt() || Model.Status == OrderPaidStatusOption.UnFrozenWait.ToInt())
                        {
                            <div class="row cl">
                                <label class="form-label col-xs-3 col-sm-3">交易状态：</label>
                                <div class="formControls col-xs-3 col-sm-3">
                                    <strong>@Model.OrderPaidUnFreeze.StatusDesc</strong>
                                </div>

                                <label class="form-label col-xs-2 col-sm-2">解冻金额：</label>
                                <div class="formControls col-xs-4 col-sm-4">
                                    <strong>@Model.OrderPaidUnFreeze.Amount</strong>
                                </div>
                            </div>

                            <div class="row cl">
                                <label class="form-label col-xs-3 col-sm-3">平台流水号：</label>
                                <div class="formControls col-xs-3 col-sm-3">
                                    <strong>@Model.OrderPaidUnFreeze.SerialNumber</strong>
                                </div>

                                <label class="form-label col-xs-2 col-sm-2">交易时间：</label>
                                <div class="formControls col-xs-4 col-sm-4">
                                    <strong>@Model.OrderPaidUnFreeze.TransTime</strong>
                                </div>
                            </div>

                        }
                        else if (Model.Status == OrderPaidStatusOption.WithdrawSuccess.ToInt() || Model.Status == OrderPaidStatusOption.WithdrawWait.ToInt())
                        {
                            <div class="row cl">
                                <label class="form-label col-xs-3 col-sm-3">交易状态：</label>
                                <div class="formControls col-xs-3 col-sm-3">
                                    <strong>@Model.OrderPaidWithdraw.StatusDesc</strong>
                                </div>

                                <label class="form-label col-xs-2 col-sm-2">提现金额：</label>
                                <div class="formControls col-xs-4 col-sm-4">
                                    <strong>@Model.OrderPaidWithdraw.Amount</strong>
                                </div>
                            </div>

                            <div class="row cl">
                                <label class="form-label col-xs-3 col-sm-3">平台流水号：</label>
                                <div class="formControls col-xs-3 col-sm-3">
                                    <strong>@Model.OrderPaidWithdraw.SerialNumber</strong>
                                </div>

                                <label class="form-label col-xs-2 col-sm-2">交易时间：</label>
                                <div class="formControls col-xs-4 col-sm-4">
                                    <strong>@Model.OrderPaidWithdraw.TransTime</strong>
                                </div>
                            </div>
                        }
                        else if (Model.Status == OrderPaidStatusOption.RechargeSuccess.ToInt() || Model.Status == OrderPaidStatusOption.RechargeWait.ToInt())
                        {
                            <div class="row cl">
                                <label class="form-label col-xs-3 col-sm-3">交易状态：</label>
                                <div class="formControls col-xs-3 col-sm-3">
                                    <strong>@Model.OrderPaidRecharge.StatusDesc</strong>
                                </div>

                                <label class="form-label col-xs-2 col-sm-2">交易金额：</label>
                                <div class="formControls col-xs-4 col-sm-4">
                                    <strong>@Model.TransactionAmount</strong>
                                </div>
                            </div>

                            <div class="row cl">
                                <label class="form-label col-xs-3 col-sm-3">交易流水号：</label>
                                <div class="formControls col-xs-3 col-sm-3">
                                    <strong>@Model.OrderPaidRecharge.TransNumber</strong>
                                </div>

                                <label class="form-label col-xs-2 col-sm-2">交易卡号：</label>
                                <div class="formControls col-xs-4 col-sm-4">
                                    <strong>@Model.OrderPaidRecharge.BankCardNumber</strong>
                                </div>
                            </div>

                            <div class="row cl">
                                <label class="form-label col-xs-3 col-sm-3">交易时间：</label>
                                <div class="formControls col-xs-3 col-sm-3">
                                    <strong>@Model.OrderPaidRecharge.TransTime</strong>
                                </div>
                            </div>
                        }
                        else
                        {
                            <div class="row c1">
                                <label>无数据信息</label>
                            </div>
                        }
                    </div>
                }
                else if (Model.Merchant.AccountBank == AccountBankOption.PingAn.ToInt())
                {
                    <div class="panel-body">

                        @if (Model.Status == OrderPaidStatusOption.Repeal.ToInt() || Model.Status == OrderPaidStatusOption.Cancel.ToInt() || Model.Status == OrderPaidStatusOption.PaySuccess.ToInt() || Model.Status == OrderPaidStatusOption.PayWait.ToInt())
                        {
                            <div class="row cl">
                                <label class="form-label col-xs-3 col-sm-3">交易状态：</label>
                                <div class="formControls col-xs-3 col-sm-3">
                                    <strong>@Model.StatusDesc</strong>
                                </div>

                                <label class="form-label col-xs-2 col-sm-2">交易金额：</label>
                                <div class="formControls col-xs-4 col-sm-4">
                                    <strong>@Model.TransactionAmount</strong>
                                </div>
                            </div>
                        }
                        else if (Model.Status == OrderPaidStatusOption.FrozenSuccess.ToInt())
                        {
                            <div class="row cl">
                                <label class="form-label col-xs-3 col-sm-3">交易状态：</label>
                                <div class="formControls col-xs-3 col-sm-3">
                                    <strong>@Model.StatusDesc</strong>
                                </div>

                                <label class="form-label col-xs-2 col-sm-2">冻结时间：</label>
                                <div class="formControls col-xs-4 col-sm-4">
                                    <strong>@Model.PingAnOrderPaid.FreezeSuccessTime</strong>
                                </div>
                            </div>
                        }
                        else if (Model.Status == OrderPaidStatusOption.UnFrozenSuccess.ToInt() || Model.Status == OrderPaidStatusOption.UnFrozenWait.ToInt())
                        {
                            <div class="row cl">
                                <label class="form-label col-xs-3 col-sm-3">交易状态：</label>
                                <div class="formControls col-xs-3 col-sm-3">
                                    <strong>@Model.StatusDesc</strong>
                                </div>

                                <label class="form-label col-xs-2 col-sm-2">解冻金额：</label>
                                <div class="formControls col-xs-4 col-sm-4">
                                    <strong>@Model.PingAnOrderPaidUnFreeze.Amount</strong>
                                </div>
                            </div>

                            <div class="row cl">
                                <label class="form-label col-xs-3 col-sm-3">平台流水号：</label>
                                <div class="formControls col-xs-3 col-sm-3">
                                    <strong>@Model.PingAnOrderPaidUnFreeze.SerialNumber</strong>
                                </div>

                                <label class="form-label col-xs-2 col-sm-2">交易时间：</label>
                                <div class="formControls col-xs-4 col-sm-4">
                                    <strong>@Model.PingAnOrderPaidUnFreeze.CreateDatetime</strong>
                                </div>
                            </div>

                        }
                        else if (Model.Status == OrderPaidStatusOption.SingleOntimePaySuccess.ToInt() || Model.Status == OrderPaidStatusOption.SingleOntimePayWait.ToInt())
                        {
                            <div class="row cl">
                                <label class="form-label col-xs-3 col-sm-3">交易状态：</label>
                                <div class="formControls col-xs-3 col-sm-3">
                                    <strong>@Model.StatusDesc</strong>
                                </div>

                                <label class="form-label col-xs-2 col-sm-2">交易金额：</label>
                                <div class="formControls col-xs-4 col-sm-4">
                                    <strong>@Model.PingAnOrderPaidRecharge.Amount</strong>
                                </div>
                            </div>

                            <div class="row cl">
                                <label class="form-label col-xs-3 col-sm-3">交易流水号：</label>
                                <div class="formControls col-xs-3 col-sm-3">
                                    <strong>@Model.PingAnOrderPaidRecharge.TransNumber</strong>
                                </div>

                                <label class="form-label col-xs-2 col-sm-2">交易卡号：</label>
                                <div class="formControls col-xs-4 col-sm-4">
                                    <strong>@Model.PingAnOrderPaidRecharge.BankCardNumber</strong>
                                </div>
                            </div>

                            <div class="row cl">
                                <label class="form-label col-xs-3 col-sm-3">清算时间：</label>
                                <div class="formControls col-xs-3 col-sm-3">
                                    <strong>@Model.PingAnOrderPaidRecharge.TransTime</strong>
                                </div>
                            </div>
                        }
                        else
                        {
                            <div class="row c1">
                                <label>无数据信息</label>
                            </div>
                        }
                    </div>
                }
            </div>
        </div>
    </form>
</div>









</head>
<body>
    @Html.Partial("_Bread")

    @RenderBody()




    <!--加载等待图标-->
    <div class="icon-position" id="LoadIcon" style="display:none;">
        <i class="icon-spinner icon-spin icon-4x"></i>
    </div>

    @Html.Partial("Tip")


    <script type="text/javascript" src="/Scripts/My97DatePicker/WdatePicker.js"></script>
    <script type="text/javascript" src="/Scripts/Common.js"></script>

    <script type="text/javascript" src="/Scripts/layer/2.1/layer.js"></script>
    <script type="text/javascript" src="/Scripts/layer/2.1/extend/layer.ext.js"></script>
    <script type="text/javascript" src="/Content/icheck/jquery.icheck.min.js"></script>
    <script type="text/javascript" src="/static/h-ui/js/H-ui.js"></script>
    <script type="text/javascript" src="/static/h-ui.admin/js/H-ui.admin.js"></script>


@using AIPXiaoTong.Model.Admin;
@using AIPXiaoTong.Model;

@{

}
@model MemberDM

<div class="page-container page-container-custom">
    <form action="" method="post" class="form form-horizontal" id="CreateFrom">
        @Html.HiddenFor(t => t.ID)

        <div class="panel panel-default">
            <div class="panel-header">基本信息</div>
            @if (Model.AccountGuangDa != null)
            {
                <div class="panel-body">
                    <div class="row cl">
                        <label class="form-label col-xs-3 col-sm-3">会员姓名：</label>
                        <div class="formControls col-xs-3 col-sm-3">
                            <strong>@Model.Name</strong>
                        </div>

                        <label class="form-label col-xs-2 col-sm-2">手机号码：</label>
                        <div class="formControls col-xs-4 col-sm-4">
                            <strong>@Model.Mobile</strong>
                        </div>
                    </div>

                    <div class="row cl">
                        <label class="form-label col-xs-3 col-sm-3">身份证：</label>
                        <div class="formControls col-xs-3 col-sm-3">
                            <strong>@Model.IDNumber</strong>
                        </div>

                        <label class="form-label col-xs-2 col-sm-2">身份证有效期：</label>
                        <div class="formControls col-xs-4 col-sm-4">
                            <strong>@Model.IdExpiredDate</strong>
                        </div>
                    </div>

                    <div class="row cl">
                        <label class="form-label col-xs-3 col-sm-3">客户所在省份：</label>
                        <div class="formControls col-xs-3 col-sm-3">
                            <strong>@Model.ProvinceName</strong>
                        </div>

                        <label class="form-label col-xs-2 col-sm-2">客户所在城市：</label>
                        <div class="formControls col-xs-4 col-sm-4">
                            <strong>@Model.CityName</strong>
                        </div>
                    </div>

                    <div class="row cl">
                        <label class="form-label col-xs-3 col-sm-3">可用余额：</label>
                        <div class="formControls col-xs-3 col-sm-3">
                            <strong>@Model.GuangDaBalance</strong>
                        </div>

                        <label class="form-label col-xs-2 col-sm-2">冻结余额：</label>
                        <div class="formControls col-xs-4 col-sm-4">
                            <strong>@Model.GuangDaFreezeBalance</strong>
                        </div>
                    </div>

                    <div class="row cl">
                        @if (Model.IsCreateAccount == 0)
                        {
                            <label class="form-label col-xs-3 col-sm-3">是否开户成功：</label>
                            <div class="formControls col-xs-3 col-sm-3">
                                <strong>否</strong>
                            </div>
                        }
                        else
                        {
                            <label class="form-label col-xs-3 col-sm-3">是否开户成功：</label>
                            <div class="formControls col-xs-3 col-sm-3">
                                <strong>否</strong>
                            </div>
                        }

                        <label class="form-label col-xs-2 col-sm-2">邮编：</label>
                        <div class="formControls col-xs-4 col-sm-4">
                            <strong>@Model.PostCode</strong>
                        </div>
                    </div>

                    <div class="row cl">
                        <label class="form-label col-xs-3 col-sm-3">联系地址：</label>
                        <div class="formControls col-xs-3 col-sm-3">
                            <strong>@Model.Address</strong>
                        </div>
                    </div>

                </div>
            }
            else if (Model.AccountPingAn != null)
            {
                <div class="panel-body">

                    <div class="row cl">
                        <label class="form-label col-xs-3 col-sm-3">会员姓名：</label>
                        <div class="formControls col-xs-3 col-sm-3">
                            <strong>@Model.Name</strong>
                        </div>

                        <label class="form-label col-xs-2 col-sm-2">手机号码：</label>
                        <div class="formControls col-xs-4 col-sm-4">
                            <strong>@Model.Mobile</strong>
                        </div>
                    </div>

                    <div class="row cl">
                        <label class="form-label col-xs-3 col-sm-3">身份证：</label>
                        <div class="formControls col-xs-3 col-sm-3">
                            <strong>@Model.IDNumber</strong>
                        </div>

                        <label class="form-label col-xs-2 col-sm-2">身份证有效期：</label>
                        <div class="formControls col-xs-4 col-sm-4">
                            <strong>@(Model.PingAnIdExpiredDate == null ? "" : Model.PingAnIdExpiredDate.Value.ToString("yyyy-MM-dd"))</strong>
                        </div>
                    </div>

                    <div class="row cl">
                        <label class="form-label col-xs-3 col-sm-3">可用余额：</label>
                        <div class="formControls col-xs-3 col-sm-3">
                            <strong>@Model.PingAnBalance</strong>
                        </div>

                        <label class="form-label col-xs-2 col-sm-2">冻结余额：</label>
                        <div class="formControls col-xs-4 col-sm-4">
                            <strong>@Model.PingAnFreezeBalance</strong>
                        </div>
                    </div>

                    <div class="row cl">
                        <label class="form-label col-xs-3 col-sm-3">平安二类户卡号：</label>
                        <div class="formControls col-xs-3 col-sm-3">
                            <strong>@Model.PingAnBankCardNumber</strong>
                        </div>

                        <label class="form-label col-xs-2 col-sm-2">POS出金卡号：</label>
                        <div class="formControls col-xs-4 col-sm-4">
                            <strong>@Model.PingAnOutCardNo</strong>
                        </div>
                    </div>

                </div>

            }

        </div>

        <div class="panel panel-default" style="margin-top:30px;">
            <div class="panel-header">相关图片</div>
            <div class="panel-body">
                <div id="layer-photos">
                    @if (Model.AccountGuangDa != null)
                    {
                        if (Model.IDImageFront != null && Model.IDImageOpposite != null)
                        {
                            <div class="row cl">
                                <label class="form-label col-xs-3 col-sm-3">身份证(正面)：</label>
                                <div class="formControls col-xs-3 col-sm-3">
                                    <img src="@(GlobalConfig.WebConfig.SiteImageUrl + Model.IDImageFront)" layer-src="@(GlobalConfig.WebConfig.SiteImageUrl + Model.IDImageFront)" width="200" height="200" />
                                </div>

                                <label class="form-label col-xs-2 col-sm-2">身份证(反面)：</label>
                                <div class="formControls col-xs-4 col-sm-4">
                                    <img src="@(GlobalConfig.WebConfig.SiteImageUrl + Model.IDImageOpposite)" layer-src="@(GlobalConfig.WebConfig.SiteImageUrl + Model.IDImageOpposite)" width="200" height="200" />
                                </div>
                            </div>
                        }
                    }
                    else if (Model.AccountPingAn != null)
                    {
                        if (Model.PingAnIDImageFront != null && Model.PingAnIDImageOpposite != null)
                        {
                            <div class="row cl">
                                <label class="form-label col-xs-3 col-sm-3">身份证(正面)：</label>
                                <div class="formControls col-xs-3 col-sm-3">
                                    <img src="@(GlobalConfig.WebConfig.SiteImageUrl + Model.PingAnIDImageFront.Replace("\\","/"))" layer-src="@(GlobalConfig.WebConfig.SiteImageUrl + Model.PingAnIDImageFront.Replace("\\","/"))" width="200" height="200" />
                                </div>

                                <label class="form-label col-xs-2 col-sm-2">身份证(反面)：</label>
                                <div class="formControls col-xs-4 col-sm-4">
                                    <img src="@(GlobalConfig.WebConfig.SiteImageUrl + Model.PingAnIDImageOpposite.Replace("\\","/"))" layer-src="@(GlobalConfig.WebConfig.SiteImageUrl + Model.PingAnIDImageOpposite.Replace("\\","/"))" width="200" height="200" />
                                </div>
                            </div>
                        }
                    }

                </div>
            </div>
        </div>

    </form>
</div>






